package com.vkp;

public class Calculator {
	public int a,b;
	public static int add(int a,int b)
	{
		return a+b;
	}
	public static int substract(int a,int b)
	{
		if(a>b)
		return a-b;
		else 
			return b-a;
	}
	public static int Add(int a,int b)
	{
		return a+b;
	}
	public static int Add(int a,int b)
	{
		return a+b;
	}
	public static void main(String[] args) {
		
	}

}
